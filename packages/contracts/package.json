{
  "name": "contracts",
  "license": "GPT-3",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "----- DEV -----": "---------------------------",
    "prepare": "chmod u+x git-install.sh && mud types",
    "git:install": "bash git-install.sh",
    "lint": "yarn prettier && yarn solhint",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "test:forge": "mud test",
    "test:hh": "run-p -l anvil:automine test:hardhat",
    "test:hardhat": "NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' yarn hardhat test --network anvil",
    "anvil:automine": "anvil --block-base-fee-per-gas 0",
    "// Hardhat skipped in GH action until hardhat officially supports TS/ESM": "https://github.com/NomicFoundation/hardhat/pull/3156",
    "test": "yarn test:forge",
    "----- BUILD/DEPLOY -----": "---------------------------",
    "start": "run-p -l hardhat:node deploy:hardhat",
    "deploy:anvil": "mud types && mud codegen-libdeploy --out src/test && mud deploy --deployerPrivateKey 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
    "deploy:hardhat": "wait-on tcp:8545 && yarn hardhat:resetfee && mud deploy --deployerPrivateKey 0x044C7963E9A89D4F8B64AB23E02E97B2E00DD57FCB60F316AC69B77135003AEF && yarn hardhat:resetfee && yarn types",
    "deploy:testnet": "mud deploy-contracts --rpc https://follower.testnet-chain.linfra.xyz --deployerPrivateKey",
    "types": "yarn mud types",
    "anvil:node": "mud devnode",
    "hardhat:node": "hardhat node",
    "hardhat:resetfee": "curl -X POST localhost:8545 -H \"Content-Type: application/json\" --data '{\"jsonrpc\": \"2.0\", \"method\": \"hardhat_setNextBlockBaseFeePerGas\", \"params\": [\"0x0\"], \"id\": 1 }'"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.6.0",
    "@ethersproject/bytes": "^5.6.0",
    "@ethersproject/providers": "^5.6.1",
    "@flatten-js/interval-tree": "^1.0.19",
    "@latticexyz/cli": "1.31.2",
    "@latticexyz/noise": "1.31.2",
    "@latticexyz/solecs": "1.31.2",
    "@latticexyz/std-contracts": "1.31.2",
    "@latticexyz/utils": "1.31.2",
    "@manifoldxyz/royalty-registry-solidity": "https://github.com/manifoldxyz/royalty-registry-solidity.git#c5ad6269d37e180fbf449a1f5ba2d2ff6c441efc",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@opengsn/contracts": "^2.2.6",
    "@rari-capital/solmate": "https://github.com/rari-capital/solmate.git#851ea3baa4327f453da723df75b1093b58b964dc",
    "@typechain/ethers-v5": "^10.1.1",
    "@types/chai": "^4.3.3",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.21",
    "abdk-libraries-solidity": "^3.0.0",
    "base64-sol": "https://github.com/Brechtpd/base64.git#4d85607b18d981acff392d2e99ba654305552a97",
    "chai": "^4.3.6",
    "copyfiles": "^2.4.1",
    "ds-test": "https://github.com/dapphub/ds-test.git#c7a36fb236f298e04edf28e2fee385b80f53945f",
    "ejs": "^3.1.8",
    "ethers": "^5.6.6",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#4d36e3f7e2168c8155c641eb0f80e85cd584bd1c",
    "glob": "^8.0.3",
    "hardhat": "https://gitpkg.now.sh/latticexyz/hardhat/packages/hardhat-core?build",
    "hardhat-deploy": "^0.11.12",
    "memmove": "https://github.com/brockelmore/memmove.git#d577ecd1bc43656f4032edf4daa9797f756a8ad2",
    "mocha": "^10.0.0",
    "node-jq": "^2.3.4",
    "npm-run-all": "^4.1.5",
    "openzeppelin-solidity": "https://github.com/OpenZeppelin/openzeppelin-contracts.git#57725120581e27ec469e1c7e497a4008aafff818",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "rimraf": "^3.0.2",
    "run-pty": "^3.0.0",
    "solhint": "^3.3.7",
    "ts-mocha": "^10.0.0",
    "ts-node": "10.7",
    "typechain": "^8.1.1",
    "typescript": "^4.6.2",
    "wait-on": "^6.0.1"
  },
  "dependencies": {}
}
